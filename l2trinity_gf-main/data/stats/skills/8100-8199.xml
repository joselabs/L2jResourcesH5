  <set name="operateType" val="OP_PASSIVE"/>
  <for>
    <mul order="0x30" stat="pAtk" val="1.04"/> <!-- P. Atk. +4% -->
    <mul order="0x30" stat="sleepVuln" val="0.3"/> <!-- Sleep Resistance +70% -->
