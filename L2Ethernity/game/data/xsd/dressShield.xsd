<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="list">
		<xs:complexType>
			<xs:sequence minOccurs="1" maxOccurs="1">
				<xs:element name="shield" maxOccurs="9999" minOccurs="0">
					<xs:complexType>
						<xs:sequence minOccurs="1" maxOccurs="1">
							<xs:element name="price" maxOccurs="1" minOccurs="0">
								<xs:complexType>
									<xs:attribute name="id" use="required" type="xs:int" />
									<xs:attribute name="count" use="required" type="xs:int" />
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="number" use="required" type="xs:int" />
						<xs:attribute name="id" use="required" type="xs:int" />
						<xs:attribute name="name" use="required" type="xs:normalizedString" />
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>