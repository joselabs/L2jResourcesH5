									<xs:enumeration value="TaxZone" />
									<xs:enumeration value="TerritoryWarZone" />
									<xs:enumeration value="TownZone" />
									<xs:enumeration value="WaterZone" />
									<xs:enumeration value="FunPvpZone" />
									<xs:enumeration value="LevelLimitZone" />
									<xs:enumeration value="NoGeoZone" />
									<xs:enumeration value="ReflectionZone" />
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="enabled" type="xs:boolean" use="required" />
		</xs:complexType>
	</xs:element>
</xs:schema>