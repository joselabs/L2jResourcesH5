												<xs:enumeration value="reviveDelay" />
												<xs:enumeration value="giveNoblesse" />
												<xs:enumeration value="bannedItems" />
												<xs:enumeration value="bannedSkills" />
												<xs:enumeration value="bannedGrades" />
												<xs:enumeration value="bannedClasses" />
												<xs:enumeration value="respawnRadius" />
												<xs:enumeration value="enchantLimit" />
												<xs:enumeration value="removeBuffs" />
												<xs:enumeration value="removePets" />
												<xs:enumeration value="isNoRestartZone" />
												<xs:enumeration value="isNoSummonFriendZone" />
												<xs:enumeration value="isNoStoreZone" />
												<xs:enumeration value="isNoLogoutZone" />
												<xs:enumeration value="reviveNoblesse" />
												<xs:enumeration value="reviveHeal" />
												<xs:enumeration value="rewardLvls" />
